<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="a207project.fall18.gamecenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GameCenter"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="a207project.fall18.gamecenter.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="a207project.fall18.gamecenter.SignUpActivity"></activity>
        <activity android:name=".GameCenterActivity"></activity>
        <activity android:name="a207project.fall18.gamecenter.StartingActivity"></activity>
        <activity android:name="a207project.fall18.gamecenter.GameActivity"></activity>
        <activity android:name=".WinActivity"></activity>
        <activity android:name="a207project.fall18.gamecenter.ScoreActivity"></activity>
        <activity android:name="a207project.fall18.gamecenter.Main2048Activity"></activity>
        <activity android:name="a207project.fall18.gamecenter.SnakeMenu"></activity>
        <activity android:name="a207project.fall18.gamecenter.SnakeActivity"></activity>
    </application>

</manifest>